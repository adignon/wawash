{
  "expo": {
    "name": "Waklin",
    "slug": "waklin",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/images/app/ios-dark.png",
        "light": "./assets/images/app/ios-light.png",
        "tinted": "./assets/images/app/ios-tinted.png"
      },
      "bundleIdentifier": "com.adignon.waklin",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app/adaptive-icon.png",
        "monochromeImage": "./assets/images/app/adaptive-icon-monochrome.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.adignon.waklin"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00BBDC",
          "dark": {
            "image": "./assets/images/app/splash-icon-light.png",
            "backgroundColor": "#00292D"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d5f90879-40e0-49b1-85d6-a18d950b6e7f"
      }
    }
  }
}
